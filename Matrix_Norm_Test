    "n = 3\n",
    "alpha = 0.99\n",
    "\n",
    "# Sample N number of PD matrices with eigenvalues 0 <= \\lambda < 1:\n",
    "\n",
    "N = 1000\n",
    "matrix_array = np.zeros((n, n, N))\n",
    "norms_array = np.zeros((N, 2))\n",
    "for k in range(N):\n",
    "    matrix_array[:, :, k] = sample_pd_matrix(n, lambda_min = 1E-3, lambda_max = 1.0)\n",
    "    norms_array[k, 0] = np.linalg.matrix_norm(matrix_array[:, :, k], ord=2)\n",
    "    norms_array[k, 1] = np.linalg.matrix_norm(matrix_array[:, :, k], ord=np.inf)\n",
    "\n",
    "norms_array"
