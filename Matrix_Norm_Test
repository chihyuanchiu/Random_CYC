    "# Norm Test"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "50d47cce",
   "metadata": {},
   "outputs": [],
   "source": [
    "# norm(X, “inf”)\n",
    "\n",
    "n = 2\n",
    "X = cp.Variable((n, n), symmetric = True)\n",
    "constraints = []\n",
    "constraints += [X >> 0]\n",
    "constraints += [X << 0.99 * np.eye(n)]\n",
    "obj = cp.Maximize(cp.norm(X, \"inf\"))\n",
    "prob = cp.Problem(obj, constraints)\n",
    "prob.solve()\n",
    "\n",
    "print(\"The optimal value is\", prob.value)\n",
    "print(\"A solution X is\")\n",
    "print(X.value)\n",
    "\n"
   ]
  },
