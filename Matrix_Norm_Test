   "source": [
    "def sample_pd_matrix(n, lambda_min = 1E-3, lambda_max = 1.0):\n",
    "    M = np.random.randn(n, n)\n",
    "    Q, _ = np.linalg.qr(M)\n",
    "    eigenvalues = np.random.uniform(lambda_min, lambda_max, n)\n",
    "    \n",
    "    return Q @ np.diag(eigenvalues) @ Q.T\n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
